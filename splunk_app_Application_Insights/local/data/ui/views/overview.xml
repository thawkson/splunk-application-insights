<form>
  <label>Overview</label>
  <fieldset submitButton="false">
    <input type="time" token="myTime" searchWhenChanged="true">
      <label>Select a Time Range:</label>
      <default>
        <earliest>-60m@m</earliest>
        <latest>now</latest>
      </default>
    </input>
    <input type="dropdown" token="refresh_token" searchWhenChanged="true">
      <label>Select a Refresh Rate:</label>
      <choice value="30s">30 Seconds</choice>
      <choice value="1m">1 Minute</choice>
      <choice value="5m">5 Minutes</choice>
      <choice value="1h">1 Hour</choice>
      <choice value="0">Off</choice>
      <default>0</default>
      <initialValue>0</initialValue>
    </input>
    <input type="link" token="timerange" searchWhenChanged="true">
      <label>Time Range Picker</label>
      <choice value="show">Show</choice>
      <choice value="hide">Hide</choice>
      <change>
        <condition value="show">
          <set token="showTimeline">true</set>
          <unset token="hideTimeline"></unset>
        </condition>
        <condition value="hide">
          <set token="hideTimeline"></set>
          <unset token="showTimeline"></unset>
        </condition>
      </change>
      <default>show</default>
      <initialValue>show</initialValue>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Timeline</title>
      <chart>
        <search>
          <query>* | timechart count</query>
          <earliest>$myTime.earliest$</earliest>
          <latest>$myTime.latest$</latest>
        </search>
        <option name="charting.chart">area</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <html>
      <center>
        <table border="0" width="100%">
          <tr>
            <td width="33%" height="100px" align="center">
                <a href="./load_balancers" target="_blank">
                  <img src="/static/app/splunk_app_Application_Insights/load-balancer-1.png" style="width:100px%;height:100px;"/>
                  <br/>
                  <strong>Load Balancers</strong> 
                </a>
            </td>
            <td width="33%" align="center">
              <a href="./web_servers" target="_blank">
                <img src="/static/app/splunk_app_Application_Insights/web-server.png" style="width:100px;height:100px;"/>
                <br/>
                <strong>Web Servers</strong> 
                </a>
            </td>
            <td width="33%" align="center">
              <a href="./app_servers" target="_blank">  
                <img src="/static/app/splunk_app_Application_Insights/application-server.png" style="width:100px;height:100px;"/>
                <br/>
                <strong>Application Servers</strong> 
                </a>
            </td>
          </tr>
        </table>
      </center>
    </html>
    </panel>
  </row>
  <row>
    <panel>
      <title>need separate charts for Performance / Errors / Traffic</title>
      <chart>
        <search>
          <query>* | timechart count</query>
          <earliest>$myTime.earliest$</earliest>
          <latest>$myTime.latest$</latest>
        </search>
        <option name="charting.chart">area</option>
      </chart>
    </panel>
    <panel>
      <title>Web Server1</title>
      <chart>
        <search>
          <query>* | timechart count</query>
          <earliest>$myTime.earliest$</earliest>
          <latest>$myTime.latest$</latest>
        </search>
        <option name="charting.chart">area</option>
      </chart>
    </panel>
    <panel>
      <title>App Server1</title>
      <chart>
        <search>
          <query>* | timechart count</query>
          <earliest>$myTime.earliest$</earliest>
          <latest>$myTime.latest$</latest>
        </search>
        <option name="charting.chart">area</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <html>
      <center>
        <table border="3" width="100%">
          <tr>
            <td width="100%" align="center">
                <img src="/static/app/splunk_app_Application_Insights/operating-systems.png" style="width:300px;height:100px;"/>
                <br/>
                <strong>Operating Systems</strong> 
            </td>
          </tr>
        </table>
      </center>
    </html>
    </panel>
  </row>
  <row>
